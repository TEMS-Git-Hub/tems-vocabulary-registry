@prefix tems:   <https://w3id.org/tems/core#> .
@prefix schema: <http://schema.org/> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .

tems: a owl:Ontology ;
  dct:title "TEMS Core Ontology (sample)"@en ;
  owl:versionInfo "0.1.0" ;
  dct:creator "TEMS" ;
  dct:license <https://creativecommons.org/licenses/by/4.0/> .

tems:MediaObject a owl:Class ;
  dct:description "A media object published in TEMS."@en ;
  rdfs:subClassOf schema:CreativeWork .

tems:hasFormat a owl:ObjectProperty ;
  rdfs:domain tems:MediaObject ;
  rdfs:range schema:MediaObject ;
  dct:description "Links to a concrete media representation."@en .