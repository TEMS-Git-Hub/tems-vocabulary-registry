@prefix tc: <http://tems.org/2024/temscore#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://tems.org/2024/temscore#> .

<http://tems.org/2024/temscore#> rdf:type owl:Ontology ;
                                  dcterms:creator "TEMS T2.3"@en ;
                                  dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
                                  rdfs:comment "An ontology for the Trusted European Media Data Space"@en ;
                                  rdfs:label "TEMSCore Ontology"@en ;
                                  owl:versionInfo "5.0.0"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://tems.org/2024/temscore#accessType
tc:accessType rdf:type owl:ObjectProperty ;
              rdfs:domain tc:Publication ;
              rdfs:range skos:Concept ;
              rdfs:comment "Access category for the publication (e.g., free, subscription, pay-per-view), modeled as a SKOS concept."@en ;
              rdfs:label "access type"@en .


###  http://tems.org/2024/temscore#contributionAgent
tc:contributionAgent rdf:type owl:ObjectProperty ;
                     rdfs:domain tc:Contribution ;
                     rdfs:range tc:Agent ;
                     rdfs:comment "Agent who contributed in this contribution."@en ;
                     rdfs:label "contribution agent"@en .


###  http://tems.org/2024/temscore#contributionRole
tc:contributionRole rdf:type owl:ObjectProperty ;
                    rdfs:domain tc:Contribution ;
                    rdfs:range tc:Role ;
                    rdfs:comment "Role performed in this contribution."@en ;
                    rdfs:label "contribution role"@en .


###  http://tems.org/2024/temscore#embeddings
tc:embeddings rdf:type owl:ObjectProperty ;
              rdfs:domain tc:Descriptor ;
              rdfs:range rdf:List ;
              rdfs:comment "A sequence of floating-point numbers representing the embeddings or vector representations of the content. The list's types will be validated via SHACL."@en ;
              rdfs:label "embeddings"@en .


###  http://tems.org/2024/temscore#evaluatesMediaObject
tc:evaluatesMediaObject rdf:type owl:ObjectProperty ;
                        rdfs:domain tc:MediaEvaluation ;
                        rdfs:range tc:MediaObject ;
                        rdfs:comment "Links the evaluation to the specific media object being evaluated."@en ;
                        rdfs:label "evaluates media object"@en .


###  http://tems.org/2024/temscore#has3DModelTechnicalMetadata
tc:has3DModelTechnicalMetadata rdf:type owl:ObjectProperty ;
                               owl:inverseOf tc:is3DModelTechnicalMetadataFor ;
                               rdfs:domain tc:MediaInstance ;
                               rdfs:range tc:ThreeDModelTechnicalMetadata ;
                               rdfs:comment "Links 3D model content to its specific technical metadata."@en ;
                               rdfs:label "has 3d model technical metadata"@en .


###  http://tems.org/2024/temscore#hasAudioTechnicalMetadata
tc:hasAudioTechnicalMetadata rdf:type owl:ObjectProperty ;
                             owl:inverseOf tc:isAudioTechnicalMetadataFor ;
                             rdfs:domain tc:MediaInstance ;
                             rdfs:range tc:AudioTechnicalMetadata ;
                             rdfs:comment "Links audio content to its specific technical metadata."@en ;
                             rdfs:label "has audio technical metadata"@en .


###  http://tems.org/2024/temscore#hasCanonicalVersion
tc:hasCanonicalVersion rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf tc:hasVersion ;
                       rdfs:domain tc:MediaWork ;
                       rdfs:range tc:MediaObject ;
                       rdfs:comment "Points from a MediaWork to its canonical/reference MediaObject (if one is designated)."@en ;
                       rdfs:label "has canonical version"@en .


###  http://tems.org/2024/temscore#hasChapter
tc:hasChapter rdf:type owl:ObjectProperty ;
              rdfs:domain tc:ContentDescription ;
              rdfs:range tc:Chapter ;
              rdfs:comment "Links a content description or descriptor to one or more chapters."@en ;
              rdfs:label "has chapter"@en .


###  http://tems.org/2024/temscore#hasConcept
tc:hasConcept rdf:type owl:ObjectProperty ;
              rdfs:domain tc:ContentDescription ;
              rdfs:range tc:Concept ;
              rdfs:comment "Associates the content with tags or categories."@en ;
              rdfs:label "has concept"@en .


###  http://tems.org/2024/temscore#hasConsumption
tc:hasConsumption rdf:type owl:ObjectProperty ;
                  owl:inverseOf tc:isConsumptionOf ;
                  rdfs:domain tc:MediaInstance ;
                  rdfs:range tc:Consumption ;
                  rdfs:comment "Links media instance to the consumption events associated with it."@en ;
                  rdfs:label "has consumption"@en .


###  http://tems.org/2024/temscore#hasContribution
tc:hasContribution rdf:type owl:ObjectProperty ;
                   owl:inverseOf tc:isContributionFor ;
                   rdfs:domain tc:MediaObject ;
                   rdfs:range tc:Contribution ;
                   rdfs:comment "Links a media object to a reified contribution (agent+role assignment)."@en ;
                   rdfs:label "has contribution"@en .


###  http://tems.org/2024/temscore#hasDescription
tc:hasDescription rdf:type owl:ObjectProperty ;
                  owl:inverseOf tc:isDescriptionOf ;
                  rdfs:domain tc:MediaObject ;
                  rdfs:range tc:ContentDescription ;
                  rdfs:comment "Links media object to its content description."@en ;
                  rdfs:label "has description"@en .


###  http://tems.org/2024/temscore#hasDescriptor
tc:hasDescriptor rdf:type owl:ObjectProperty ;
                 owl:inverseOf tc:isDescriptorOf ;
                 rdfs:domain tc:ContentDescription ;
                 rdfs:range tc:Descriptor ;
                 rdfs:comment "Links a content description to one or more descriptors for agent-dependent fields."@en ;
                 rdfs:label "has descriptor"@en .


###  http://tems.org/2024/temscore#hasEvaluationContributor
tc:hasEvaluationContributor rdf:type owl:ObjectProperty ;
                            rdfs:domain tc:MediaEvaluation ;
                            rdfs:range tc:Agent ;
                            rdfs:comment "Links media evaluation to contributors involved in fact-checking."@en ;
                            rdfs:label "has evaluation contributor"@en .


###  http://tems.org/2024/temscore#hasEvaluator
tc:hasEvaluator rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf tc:hasEvaluationContributor ;
                owl:inverseOf tc:isEvaluatorOf ;
                rdfs:domain tc:MediaEvaluation ;
                rdfs:range tc:Agent ;
                rdfs:comment "Links a media evaluation to the fact-checking agent that performed the evaluation."@en ;
                rdfs:label "has evaluator"@en .


###  http://tems.org/2024/temscore#hasEvent
tc:hasEvent rdf:type owl:ObjectProperty ;
            owl:inverseOf tc:isEventFor ;
            rdfs:domain owl:Thing ;
            rdfs:range tc:Event ;
            rdfs:comment "Links an entity, such as media object or publication, to an event."@en ;
            rdfs:label "has event"@en .


###  http://tems.org/2024/temscore#hasEventOrganizer
tc:hasEventOrganizer rdf:type owl:ObjectProperty ;
                     rdfs:domain tc:Event ;
                     rdfs:range tc:Agent ;
                     rdfs:comment "Links an event to the agent (person, organization, or tool) responsible for organizing it."@en ;
                     rdfs:label "has event organizer"@en .


###  http://tems.org/2024/temscore#hasEventType
tc:hasEventType rdf:type owl:ObjectProperty ;
                rdfs:domain tc:Event ;
                rdfs:range skos:Concept ;
                rdfs:comment "Links the event to a SKOS concept representing its type or category (e.g., political, social, entertainment)."@en ;
                rdfs:label "has event type"@en .


###  http://tems.org/2024/temscore#hasGenre
tc:hasGenre rdf:type owl:ObjectProperty ;
            rdfs:domain tc:ContentDescription ;
            rdfs:range skos:Concept ;
            rdfs:comment "Genre/type linked to an external concept (e.g., IPTC genre, ISAN Work Type)."@en ;
            rdfs:label "has genre"@en .


###  http://tems.org/2024/temscore#hasGrouping
tc:hasGrouping rdf:type owl:ObjectProperty ;
               owl:inverseOf tc:isGroupingOf ;
               rdfs:domain tc:MediaObject ;
               rdfs:range tc:ContentGrouping ;
               rdfs:comment "Associates media object with a content grouping that categorizes or organizes it."@en ;
               rdfs:label "has grouping"@en .


###  http://tems.org/2024/temscore#hasIdentifier
tc:hasIdentifier rdf:type owl:ObjectProperty ;
                 owl:inverseOf tc:isIdentifierOf ;
                 rdfs:range tc:Identifier ;
                 rdfs:comment "Associates a resource with one of its identifiers."@en ;
                 rdfs:label "has identifier"@en .


###  http://tems.org/2024/temscore#hasImageTechnicalMetadata
tc:hasImageTechnicalMetadata rdf:type owl:ObjectProperty ;
                             owl:inverseOf tc:isImageTechnicalMetadataFor ;
                             rdfs:domain tc:MediaInstance ;
                             rdfs:range tc:ImageTechnicalMetadata ;
                             rdfs:comment "Links image content to its specific technical metadata."@en ;
                             rdfs:label "has image technical metadata"@en .


###  http://tems.org/2024/temscore#hasInstance
tc:hasInstance rdf:type owl:ObjectProperty ;
               owl:inverseOf tc:isInstanceOf ;
               rdfs:domain tc:MediaObject ;
               rdfs:range tc:MediaInstance ;
               rdfs:comment "Indicates that a MediaObject has a specific MediaInstance."@en ;
               rdfs:label "has instance"@en .


###  http://tems.org/2024/temscore#hasIntendedAudience
tc:hasIntendedAudience rdf:type owl:ObjectProperty ;
                       rdfs:domain tc:MediaObject ;
                       rdfs:range tc:TargetAudience ;
                       rdfs:comment "Editorially intended audience for the media object (default audience independent of channel/publication). A publication may override this via hasTargetAudience."@en ;
                       rdfs:label "has intended audience"@en .


###  http://tems.org/2024/temscore#hasLocation
tc:hasLocation rdf:type owl:ObjectProperty ;
               rdfs:domain owl:Thing ;
               rdfs:range tc:Location ;
               rdfs:comment "Links an entity, such as media object or an agent, to its geographic location."@en ;
               rdfs:label "has location"@en .


###  http://tems.org/2024/temscore#hasProductionEvent
tc:hasProductionEvent rdf:type owl:ObjectProperty ;
                      rdfs:domain tc:ProductionJob ;
                      rdfs:range tc:Event ;
                      rdfs:comment "Links the ProductionJob to a related event in the production process."@en ;
                      rdfs:label "has production event"@en .


###  http://tems.org/2024/temscore#hasProductionJobContributor
tc:hasProductionJobContributor rdf:type owl:ObjectProperty ;
                               rdfs:domain tc:ProductionJob ;
                               rdfs:range tc:Contributors ;
                               rdfs:comment "Links the ProductionJob to a contributor involved in the production, along with their role."@en ;
                               rdfs:label "has production job contributor"@en .


###  http://tems.org/2024/temscore#hasProvenance
tc:hasProvenance rdf:type owl:ObjectProperty ;
                 owl:inverseOf tc:isProvenanceFor ;
                 rdfs:domain tc:MediaObject ;
                 rdfs:range tc:Provenance ;
                 rdfs:comment "Links media object to its provenance details."@en ;
                 rdfs:label "has provenance"@en .


###  http://tems.org/2024/temscore#hasPublication
tc:hasPublication rdf:type owl:ObjectProperty ;
                  owl:inverseOf tc:isPublicationOf ;
                  rdfs:domain tc:MediaInstance ;
                  rdfs:range tc:Publication ;
                  rdfs:comment "Links media instance to its publication details."@en ;
                  rdfs:label "has publication"@en .


###  http://tems.org/2024/temscore#hasPublisher
tc:hasPublisher rdf:type owl:ObjectProperty ;
                owl:inverseOf tc:isPublisherOf ;
                rdfs:domain tc:Publication ;
                rdfs:range tc:Agent ;
                rdfs:comment "Links a publication to its publisher."@en ;
                rdfs:label "has publisher"@en .


###  http://tems.org/2024/temscore#hasRights
tc:hasRights rdf:type owl:ObjectProperty ;
             owl:inverseOf tc:isRightsOf ;
             rdfs:domain tc:MediaObject ;
             rdfs:range tc:Rights ;
             rdfs:comment "Links media object to its rights information."@en ;
             rdfs:label "has rights"@en .


###  http://tems.org/2024/temscore#hasSegment
tc:hasSegment rdf:type owl:ObjectProperty ;
              owl:inverseOf tc:isSegmentOf ;
              rdfs:domain tc:OrderedSegment ;
              rdfs:range tc:MediaObject ;
              rdfs:comment "Links an OrderedSegment to a specific media object segment (e.g., video, audio, etc.)."@en ;
              rdfs:label "has segment"@en .


###  http://tems.org/2024/temscore#hasSegmentType
tc:hasSegmentType rdf:type owl:ObjectProperty ;
                  rdfs:domain tc:OrderedSegment ;
                  rdfs:range skos:Concept ;
                  rdfs:comment "Links an OrderedSegment to a segment type concept (e.g., Article, Image)."@en ;
                  rdfs:label "has segment type"@en .


###  http://tems.org/2024/temscore#hasTargetAudience
tc:hasTargetAudience rdf:type owl:ObjectProperty ;
                     owl:inverseOf tc:isTargetAudienceFor ;
                     rdfs:domain tc:Publication ;
                     rdfs:range tc:TargetAudience ;
                     rdfs:comment "Links the publication to its target audience."@en ;
                     rdfs:label "has target audience"@en .


###  http://tems.org/2024/temscore#hasTechnicalMetadata
tc:hasTechnicalMetadata rdf:type owl:ObjectProperty ;
                        owl:inverseOf tc:isTechnicalMetadataFor ;
                        rdfs:domain tc:MediaInstance ;
                        rdfs:range tc:TechnicalMetadata ;
                        rdfs:comment "Links the MediaInstance to its specific technical metadata."@en ;
                        rdfs:label "has technical metadata"@en .


###  http://tems.org/2024/temscore#hasTextTrackTechnicalMetadata
tc:hasTextTrackTechnicalMetadata rdf:type owl:ObjectProperty ;
                                 owl:inverseOf tc:isTextTrackTechnicalMetadataFor ;
                                 rdfs:domain tc:MediaInstance ;
                                 rdfs:range tc:TextTrackTechnicalMetadata ;
                                 rdfs:comment "Links media instance to its subtitle/caption technical metadata."@en ;
                                 rdfs:label "has text track technical metadata"@en .


###  http://tems.org/2024/temscore#hasTitle
tc:hasTitle rdf:type owl:ObjectProperty ;
            rdfs:domain tc:MediaObject ;
            rdfs:range tc:Title ;
            rdfs:comment "Links a media object to one or more title variants."@en ;
            rdfs:label "has title"@en .


###  http://tems.org/2024/temscore#hasVersion
tc:hasVersion rdf:type owl:ObjectProperty ;
              owl:inverseOf tc:isVersionOf ;
              rdfs:domain tc:MediaWork ;
              rdfs:range tc:MediaObject ;
              rdfs:comment "Relates a MediaWork to one of its editorial versions (tc:MediaObject)."@en ;
              rdfs:label "has version"@en .


###  http://tems.org/2024/temscore#hasVideoTechnicalMetadata
tc:hasVideoTechnicalMetadata rdf:type owl:ObjectProperty ;
                             owl:inverseOf tc:isVideoTechnicalMetadataFor ;
                             rdfs:domain tc:MediaInstance ;
                             rdfs:range tc:VideoTechnicalMetadata ;
                             rdfs:comment "Links video content to its specific technical metadata."@en ;
                             rdfs:label "has video technical metadata"@en .


###  http://tems.org/2024/temscore#holdsRightsFor
tc:holdsRightsFor rdf:type owl:ObjectProperty ;
                  owl:inverseOf tc:isHeldBy ;
                  rdfs:domain tc:Agent ;
                  rdfs:range tc:Rights ;
                  rdfs:comment "Indicates that an agent holds the rights for specific content."@en ;
                  rdfs:label "holds rights for"@en .


###  http://tems.org/2024/temscore#identifierScheme
tc:identifierScheme rdf:type owl:ObjectProperty ;
                    rdfs:domain tc:Identifier ;
                    rdfs:range skos:Concept ;
                    rdfs:comment "Scheme or registry of the identifier (e.g., DOI, ISAN, ISRC), modeled as a SKOS concept."@en ;
                    rdfs:label "identifier scheme"@en .


###  http://tems.org/2024/temscore#identifierType
tc:identifierType rdf:type owl:ObjectProperty ;
                  rdfs:domain tc:Identifier ;
                  rdfs:range skos:Concept ;
                  rdfs:comment "Type/category of the identifier value within a scheme (e.g., 'Print ISSN', 'Online ISSN'), modeled as a SKOS concept."@en ;
                  rdfs:label "identifier type"@en .


###  http://tems.org/2024/temscore#is3DModelTechnicalMetadataFor
tc:is3DModelTechnicalMetadataFor rdf:type owl:ObjectProperty ;
                                 rdfs:domain tc:ThreeDModelTechnicalMetadata ;
                                 rdfs:range tc:MediaInstance ;
                                 rdfs:comment "Links 3D model technical metadata to the 3D model content it describes."@en ;
                                 rdfs:label "is 3d model technical metadata for"@en .


###  http://tems.org/2024/temscore#isAudioTechnicalMetadataFor
tc:isAudioTechnicalMetadataFor rdf:type owl:ObjectProperty ;
                               rdfs:domain tc:AudioTechnicalMetadata ;
                               rdfs:range tc:MediaInstance ;
                               rdfs:comment "Links audio technical metadata to the audio content it describes."@en ;
                               rdfs:label "is audio technical metadata for"@en .


###  http://tems.org/2024/temscore#isBasedOnPublication
tc:isBasedOnPublication rdf:type owl:ObjectProperty ;
                        owl:inverseOf tc:isBasisForConsumption ;
                        rdfs:domain tc:Consumption ;
                        rdfs:range tc:Publication ;
                        rdfs:comment "Links the consumption of media object to the publication it is based on."@en ;
                        rdfs:label "is based on publication"@en .


###  http://tems.org/2024/temscore#isBasisForConsumption
tc:isBasisForConsumption rdf:type owl:ObjectProperty ;
                         rdfs:domain tc:Publication ;
                         rdfs:range tc:Consumption ;
                         rdfs:comment "Indicates that a publication serves as the basis for a consumption record."@en ;
                         rdfs:label "is basis for consumption"@en .


###  http://tems.org/2024/temscore#isConsumptionOf
tc:isConsumptionOf rdf:type owl:ObjectProperty ;
                   rdfs:domain tc:Consumption ;
                   rdfs:range tc:MediaInstance ;
                   rdfs:comment "Indicates that a consumption record is associated with a media instance."@en ;
                   rdfs:label "is consumption of"@en .


###  http://tems.org/2024/temscore#isContributionFor
tc:isContributionFor rdf:type owl:ObjectProperty ;
                     rdfs:domain tc:Contribution ;
                     rdfs:range tc:MediaObject ;
                     rdfs:comment "Inverse: the contribution belongs to a specific media object."@en ;
                     rdfs:label "is contribution for"@en .


###  http://tems.org/2024/temscore#isDescribedBy
tc:isDescribedBy rdf:type owl:ObjectProperty ;
                 owl:inverseOf tc:performsDescription ;
                 rdfs:domain tc:Descriptor ;
                 rdfs:range tc:Agent ;
                 rdfs:comment "Links a descriptor to the agent who performed the description."@en ;
                 rdfs:label "is described by"@en .


###  http://tems.org/2024/temscore#isDescriptionOf
tc:isDescriptionOf rdf:type owl:ObjectProperty ;
                   rdfs:domain tc:ContentDescription ;
                   rdfs:range tc:MediaObject ;
                   rdfs:comment "Indicates that a content description is associated with the media object it describes."@en ;
                   rdfs:label "is description of"@en .


###  http://tems.org/2024/temscore#isDescriptorOf
tc:isDescriptorOf rdf:type owl:ObjectProperty ;
                  rdfs:domain tc:Descriptor ;
                  rdfs:range tc:ContentDescription ;
                  rdfs:comment "Indicates that a descriptor is part of or belongs to a specific content description."@en ;
                  rdfs:label "is descriptor of"@en .


###  http://tems.org/2024/temscore#isEvaluatorOf
tc:isEvaluatorOf rdf:type owl:ObjectProperty ;
                 rdfs:domain tc:Agent ;
                 rdfs:range tc:MediaEvaluation ;
                 rdfs:comment "Indicates that an agent is the evaluator for a media evaluation."@en ;
                 rdfs:label "is evaluator of"@en .


###  http://tems.org/2024/temscore#isEventFor
tc:isEventFor rdf:type owl:ObjectProperty ;
              rdfs:domain tc:Event ;
              rdfs:range owl:Thing ;
              rdfs:comment "Indicates that an event is associated with a specific entity, such as media object or publication."@en ;
              rdfs:label "is event for"@en .


###  http://tems.org/2024/temscore#isGroupingOf
tc:isGroupingOf rdf:type owl:ObjectProperty ;
                rdfs:domain tc:ContentGrouping ;
                rdfs:range tc:MediaObject ;
                rdfs:comment "Indicates that the content grouping categorizes or organizes specific media object."@en ;
                rdfs:label "is grouping of"@en .


###  http://tems.org/2024/temscore#isHeldBy
tc:isHeldBy rdf:type owl:ObjectProperty ;
            rdfs:domain tc:Rights ;
            rdfs:range tc:Agent ;
            rdfs:comment "Indicates that the rights are held by a specific agent."@en ;
            rdfs:label "is held by"@en .


###  http://tems.org/2024/temscore#isIdentifierOf
tc:isIdentifierOf rdf:type owl:ObjectProperty ;
                  rdfs:domain tc:Identifier ;
                  rdfs:range owl:Thing ;
                  rdfs:comment "Inverse of has identifier."@en ;
                  rdfs:label "is identifier of"@en .


###  http://tems.org/2024/temscore#isImageTechnicalMetadataFor
tc:isImageTechnicalMetadataFor rdf:type owl:ObjectProperty ;
                               rdfs:domain tc:ImageTechnicalMetadata ;
                               rdfs:range tc:MediaInstance ;
                               rdfs:comment "Links image technical metadata to the image content it describes."@en ;
                               rdfs:label "is image technical metadata for"@en .


###  http://tems.org/2024/temscore#isInstanceOf
tc:isInstanceOf rdf:type owl:ObjectProperty ;
                rdfs:domain tc:MediaInstance ;
                rdfs:range tc:MediaObject ;
                rdfs:comment "Links a MediaInstance to the MediaObject it represents."@en ;
                rdfs:label "is instance of"@en .


###  http://tems.org/2024/temscore#isProvenanceFor
tc:isProvenanceFor rdf:type owl:ObjectProperty ;
                   rdfs:domain tc:Provenance ;
                   rdfs:range tc:MediaObject ;
                   rdfs:comment "Indicates that this provenance information is relevant for one or more media object items."@en ;
                   rdfs:label "is provenance for"@en .


###  http://tems.org/2024/temscore#isPublicationOf
tc:isPublicationOf rdf:type owl:ObjectProperty ;
                   rdfs:domain tc:Publication ;
                   rdfs:range tc:MediaInstance ;
                   rdfs:comment "Indicates that the publication is associated with specific media instance."@en ;
                   rdfs:label "is publication of"@en .


###  http://tems.org/2024/temscore#isPublisherOf
tc:isPublisherOf rdf:type owl:ObjectProperty ;
                 rdfs:domain tc:Agent ;
                 rdfs:range tc:Publication ;
                 rdfs:comment "Indicates that the agent is the publisher of a specific publication."@en ;
                 rdfs:label "is publisher of"@en .


###  http://tems.org/2024/temscore#isRightsOf
tc:isRightsOf rdf:type owl:ObjectProperty ;
              rdfs:domain tc:Rights ;
              rdfs:range tc:MediaObject ;
              rdfs:comment "Indicates that the rights are associated with specific media object."@en ;
              rdfs:label "is rights of"@en .


###  http://tems.org/2024/temscore#isSegmentOf
tc:isSegmentOf rdf:type owl:ObjectProperty ;
               rdfs:domain tc:MediaObject ;
               rdfs:range tc:OrderedSegment ;
               rdfs:comment "Indicates that a MediaObject is part of an OrderedSegment."@en ;
               rdfs:label "is segment of"@en .


###  http://tems.org/2024/temscore#isTargetAudienceFor
tc:isTargetAudienceFor rdf:type owl:ObjectProperty ;
                       rdfs:domain tc:TargetAudience ;
                       rdfs:range tc:Publication ;
                       rdfs:comment "Indicates that the target audience is the intended audience for specific publications."@en ;
                       rdfs:label "is target audience for"@en .


###  http://tems.org/2024/temscore#isTechnicalMetadataFor
tc:isTechnicalMetadataFor rdf:type owl:ObjectProperty ;
                          rdfs:domain tc:TechnicalMetadata ;
                          rdfs:range tc:MediaInstance ;
                          rdfs:comment "Links technical metadata to the media object it describes."@en ;
                          rdfs:label "is technical metadata for"@en .


###  http://tems.org/2024/temscore#isTextTrackTechnicalMetadataFor
tc:isTextTrackTechnicalMetadataFor rdf:type owl:ObjectProperty ;
                                   rdfs:domain tc:TextTrackTechnicalMetadata ;
                                   rdfs:range tc:MediaInstance ;
                                   rdfs:comment "Links subtitle/caption technical metadata to the media instance it describes."@en ;
                                   rdfs:label "is text track technical metadata for"@en .


###  http://tems.org/2024/temscore#isVersionOf
tc:isVersionOf rdf:type owl:ObjectProperty ;
               rdfs:domain tc:MediaObject ;
               rdfs:range tc:MediaWork ;
               rdfs:comment "Relates an editorial version (tc:MediaObject) to its parent MediaWork."@en ;
               rdfs:label "is version of"@en .


###  http://tems.org/2024/temscore#isVideoTechnicalMetadataFor
tc:isVideoTechnicalMetadataFor rdf:type owl:ObjectProperty ;
                               rdfs:domain tc:VideoTechnicalMetadata ;
                               rdfs:range tc:MediaInstance ;
                               rdfs:comment "Links video technical metadata to the video content it describes."@en ;
                               rdfs:label "is video technical metadata for"@en .


